            function EnviarActivacionCorreo() {
                swal({
                    title: 'Escribe tu correo por favor',
                    input: 'email',
                    showCancelButton: true,
                    confirmButtonText: 'Enviar',
                    showLoaderOnConfirm: true,
                    preConfirm: (email) => {
                        return new Promise((resolve) => {
                            setTimeout(() => {
                                if (email === 'taken@example.com') {
                                    swal.showValidationError(
                                        'This email is already taken.'
                                    )
                                }
                                resolve()
                            }, 2000)
                        })
                    },
                    allowOutsideClick: () => !swal.isLoading()
                }).then((result) => {
                    if (result.value) {
                        $('<audio id="chatAudio"><source src="notify.ogg" type="audio/ogg"><source src="/pages/audio/exito.mp3" " type="audio/mpeg"><source src="notify.wav" type="audio/wav"></audio>').appendTo('body');
                        $('#chatAudio')[0].play();
                        swal({
                            type: 'success',
                            title: 'Se te envio el correo de activación!',
                            html: 'Enviado a : ' + result.value
                        })
                    }
                })
            }

