              if (campo === 'txtContra2') {
                  documento = document.getElementById('txtDocumento').value;
                  nombres = document.getElementById('txtNombres').value;
                  apellidos = document.getElementById('txtApellidos').value;
                  correo = document.getElementById('txtCorreo').value;
