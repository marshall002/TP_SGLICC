                  else if ((contra2 == apellidos) || ((contra2 == apellidos + (/[a-zA-Z]\s{2,}/.test(contra2))))) {
                      $("#glypcn" + campo).remove();
                      $('#' + campo).parent().css({ "color": "brown" });
